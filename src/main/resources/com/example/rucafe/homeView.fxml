    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <VBox prefHeight="200.0" prefWidth="100.0">
      <children>
         <Button mnemonicParsing="false" onAction="#onDrinksButtonClick" text="Drinks" />
